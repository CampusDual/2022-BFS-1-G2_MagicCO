<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup
        xmlns="http://www.ontimize.com/schema/jdbc"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd"
        catalog="" schema="${mainschema}" table="SHOP"
        datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
    <DeleteKeys>
        <Column>id_shop</Column>
    </DeleteKeys>
    <UpdateKeys>
        <Column>id_shop</Column>
    </UpdateKeys>
    <GeneratedKey>id_shop</GeneratedKey>
    <Queries>
        <Query id="shopSubscriptionDetails">
            <AmbiguousColumns>
                <AmbiguousColumn name="shop_start_date" prefix="sh" databaseName="start_date"/>
                <AmbiguousColumn name="subscription_start_date" prefix="shsu" databaseName="start_date"/>
                <AmbiguousColumn name="id_subscription" prefix="ss" databaseName="id_subscription"/>
            </AmbiguousColumns>
            <Sentence>
                <![CDATA[
      select
      #COLUMNS#
     from
     public.subscription_service ss
     inner join public.service se on
      ss.id_service = se.id_service
     inner join public.subscription su on
      ss.id_subscription = su.id_subscription
    #WHERE#
   ]]>
            </Sentence>
        </Query>
    </Queries>
    </JdbcEntitySetup>